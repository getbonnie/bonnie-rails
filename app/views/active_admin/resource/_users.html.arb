panel 'Users' do
  table_for data.users do
    column :id
    column :name do |item|
      auto_link item, item.name
    end
    column :tags do |item|
      status_tag item.state if item.state
      status_tag item.type_of if item.type_of
    end
    column :created_at
    column :updated_at
  end
end
